{
  "language": "en",
  "dictionaries": [
    "dict"
  ],
  "dictionaryDefinitions": [
    {
      "name": "dict",
      "path": "./dict.txt",
      "addWords": true
    }
  ],
  "ignorePaths": [
    // binaries
    "**/*.a",
    "**/*.dll",
    "**/*.dylib",
    "**/*.so",
    "**/*.wav",

    // unity
    "**/*.asset",
    "**/*.meta",
    "**/*.unity",
    "**/Packages/*",
    "**/Library/*",
    "**/Logs/*",
    "**/obj/*",
    "**/*.unitypackage",
    "**/*.csproj",
    "**/ProjectSettings/*"
  ]
}